#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_@craco_craco@6.1.2@@craco/craco/bin/craco.js" "$@"
  ret=$?
else 
  node  "$basedir/../_@craco_craco@6.1.2@@craco/craco/bin/craco.js" "$@"
  ret=$?
fi
exit $ret
